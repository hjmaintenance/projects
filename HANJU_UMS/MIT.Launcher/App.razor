@using Microsoft.AspNetCore.Components.Authorization
@using MIT.UI.Main.Main
@DxResourceManager.RegisterScripts()

<CascadingAuthenticationState>
  <Router AppAssembly="@typeof(App).Assembly" AdditionalAssemblies="new[] { typeof(MainUILayoutFinal).Assembly }">
      <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainUILayoutFinal)" />
      </Found>
      <NotFound>
        <PageTitle>Not Found</PageTitle>
        <LayoutView Layout="@typeof(MainUILayout)">
          <p role="alert">Sorry, there's nothing at this address.</p>
        </LayoutView>
      </NotFound>
    </Router>
</CascadingAuthenticationState>